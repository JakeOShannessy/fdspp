&HEAD CHID='RadiationThroughGlazingDirectional'
      TITLE='Test Radiation Through Glazing;' /
&TIME T_END=20.0 /
&DUMP COLUMN_DUMP_LIMIT=.TRUE., DT_SL3D=0.25, DT_SLCF=0.25 DT_BNDF=0.25 /

&MESH ID='MainMesh', IJK=100,200,100, XB=0,1,0,2,0,1 /

&SURF ID='Heater' NET_HEAT_FLUX=10 HEAT_TRANSFER_COEFFICIENT=0 /

&VENT ID='RadiativePanel', SURF_ID='Heater', XB=0,0.4,0,0,0,1 COLOR='RED' /
&OBST ID='Obstruction', SURF_ID='INERT', XB=0.4,0.5,0,0.4,0,1 COLOR='BLUE' /

&RADI NUMBER_RADIATION_ANGLES=600 /

! &VENT ID='UnsurfaceMinY', SURF_ID='OPEN', XB=0,1,0,0,0,1 /
&VENT ID='UnsurfaceMaxY', SURF_ID='OPEN', XB=0,1,2,2,0,1 /
&VENT ID='UnsurfaceMinZ', SURF_ID='OPEN', XB=0,1,0,2,0,0 /
&VENT ID='UnsurfaceMaxZ', SURF_ID='OPEN', XB=0,1,0,2,1,1 /

&BNDF QUANTITY='RADIATIVE HEAT FLUX' /

&DEVC XB=0.99,1.01,0,1,0,1, QUANTITY='INCIDENT HEAT FLUX' ID='IncidentReceived' SPATIAL_STATISTIC='SURFACE INTEGRAL' /
&DEVC XYZ=1,0.5,0.5, QUANTITY='INCIDENT HEAT FLUX' ID='IncidentReceivedPoint' IOR=-1 /

&MATL ID='GLASS',
      SPECIFIC_HEAT=0.84,
      CONDUCTIVITY=0.76,
      EMISSIVITY=0.8,
      ABSORPTION_COEFFICIENT=1,
      DENSITY=2700.0/

&SURF ID='WINDOW',
      RGB=102,255,255,
      TRANSPARENCY=0.2,
      BACKING='EXPOSED',
      MATL_ID(1,1)='GLASS',
      MATL_MASS_FRACTION(1,1)=1.0,
      HEAT_TRANSFER_COEFFICIENT=0,
      THICKNESS(1)=0.005/

! &OBST SURF_ID='WINDOW' XB=0.5,0.5,0,1,0,1 /

&TAIL /
